ls',id).then((res) => {\n            this.orderDetails = res\n        })\n    }\n\n  },\n\n  computed: {\n    orders() {\n      return this.$store.state.orders;\n    }\n  },\n  created() {\n    this.fetchAllOrders();\n  }\n};\n</script>\n\n<template>\n  <div>\n    <admin-navbar></admin-navbar>\n    <table>\n      <tr>\n        <th>ID</th>\n        <th>Customer ID</th>\n        <th>Address</th>\n        <th>Total Amount</th>\n        <th>Status</th>\n        <th>Details</th>\n      </tr>\n\n      <tr v-for=\"order in orders\" :key=\"order.id\">\n        <td>{{ order.id }}</td>\n        <td>{{ order.customer_id }}</td>\n        <td>{{ order.address }}</td>\n        <td>{{ order.totalAmount }}</td>\n        <td>{{ order.status }}</td>\n        <td @click=\"showDetails(order.id)\" class=\"table-details\">Details</td>\n      </tr>\n    </table>\n\n    <div class=\"modalOverlay\" @click=\"showDetailsModal = false\" v-if=\"showDetailsModal\"></div>\n    <div class=\"details-modal\" v-if=\"showDetailsModal\">\n      <table>\n        <tr>\n          <th>ID</th>\n          <th>Brand</th>\n          <th>Description</th>\n          <th>Price</th>\n        </tr>\n\n        <tr v-for=\"order in orderDetails\" :key=\"order.id\">\n            <th>{{order.id}}</th>\n            <th>{{order.brand}}</th>\n            <th>{{order.description}}</th>\n            <th>{{order.price}}</th>\n        </tr>\n      </table>\n    </div>\n  </div>\n</template>\n"],"sourceRoot":"src/views/admin"}]}