    });\n    },\n\n    onAddSubmit() {\n      var addedProduct = {\n        photo: this.$refs.photo.value,\n        brand: this.$refs.brand.value,\n        description: this.$refs.description.value,\n        price: this.$refs.price.value,\n        gender: this.$refs.gender.value,\n        category_id: this.$refs.category_id.value,\n        status: this.$refs.status.value,\n        stock: this.$refs.stock.value\n      };\n\n      this.$store.dispatch(\"addProduct\", addedProduct);\n    }\n  },\n  created() {\n    this.fetchAllProducts();\n  }\n};\n</script>\n\n<template>\n  <div>\n    <admin-navbar></admin-navbar>\n    <div class=\"admin-main\">\n      <div @click=\"showAddModal = true\">\n        <add class=\"add-icon admin-card-button\"></add>\n      </div>\n      <div class=\"container\">\n        <admin-card\n          v-for=\"product in products\"\n          :key=\"product.id\"\n          :product=\"product\"\n        ></admin-card>\n\n        <div\n          class=\"modalOverlay\"\n          v-if=\"showAddModal\"\n          @click=\"showAddModal = false\"\n        ></div>\n        <div class=\"editModal\" v-if=\"showAddModal\">\n          <form\n            action=\"/api/admin/edit/\"\n            method=\"POST\"\n            @submit.prevent=\"onAddSubmit()\"\n          >\n            <div class=\"edit-item\">\n              <div class=\"edit-item-wrapper\">\n                <label for=\"photo\">Photo URL</label>\n                <input\n                  type=\"url\"\n                  ref=\"photo\"\n                  name=\"photo\"\n                  placeholder=\"Photo\"\n                />\n\n                <label for=\"brand\">Brand</label>\n                <input\n                  type=\"text\"\n                  ref=\"brand\"\n                  name=\"brand\"\n                  placeholder=\"Brand\"\n                />\n\n                <label for=\"description\">Description</label>\n                <input\n                  type=\"text\"\n                  ref=\"description\"\n                  name=\"description\"\n                  placeholder=\"Description\"\n                />\n\n                <label for=\"price\">Price</label>\n                <input\n                  type=\"number\"\n                  ref=\"price\"\n                  name=\"price\"\n                  placeholder=\"Price\"\n                />\n\n                <label for=\"category_id\">Category ID</label>\n                <input\n                  type=\"number\"\n                  ref=\"category_id\"\n                  name=\"category_id\"\n                  placeholder=\"Category ID\"\n                />\n\n                <label for=\"stock\">Stock</label>\n                <input\n                  type=\"number\"\n                  ref=\"stock\"\n                  name=\"stock\"\n                  placeholder=\"Stock\"\n                />\n\n                <label for=\"gender\">Gender</label>\n                <input\n                  type=\"number\"\n                  ref=\"gender\"\n                  name=\"gender\"\n                  placeholder=\"Gender\"\n                />\n\n                <label for=\"status\">Status</label>\n                <input\n                  type=\"number\"\n                  ref=\"status\"\n                  name=\"status\"\n                  placeholder=\"Status\"\n                />\n\n                <button type=\"submit\" class=\"btn change\">Add Item</button>\n                <button class=\"btn cancel\" @click=\"showAddModal = false\">\n                  Cancel\n                </button>\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n"]}]}